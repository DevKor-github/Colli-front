{
  "name": "uniwork-front",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint App.tsx src/ --ext .ts,.tsx",
    "format": "prettier --write App.tsx src/**/*.{ts,tsx}",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install"
